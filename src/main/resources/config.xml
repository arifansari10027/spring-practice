<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--property injection using value as tag-->
    <bean class="com.practice.Student" name="student1">
        <property name="studentId">
            <value>101</value>
        </property>
        <property name="firstName">
            <value>Ankush</value>
        </property>
        <property name="lastName">
            <value>Asad Pandey</value>
        </property>
        <property name="address">
            <value>Oman ka 14</value>
        </property>
    </bean>

    <!--property injection using value as attribute-->
    <bean class="com.practice.Student" name="student2">
        <property name="studentId" value="101"/>
        <property name="firstName" value="Arif"/>
        <property name="lastName" value="Ansari" />
        <property name="address" value="Jamshedpur" />
    </bean>

    <!--property injection using value as p schema-->
    <bean class="com.practice.Student" name="student3" p:studentId="102" p:firstName="Nazish" p:lastName="Kamran" p:address="Hyderabad" />

</beans>
