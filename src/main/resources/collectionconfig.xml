<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.practice.springcore.collections.CollectionsPractice" name="collections">

        <property name="empName" value="Arif"/>

        <property name="empPhones">
            <list>
                <value>+9164645</value>
                <value>+3515645</value>
                <value>+6544631</value>
            </list>
        </property>

        <property name="addresses">
            <set>
                <value>USA</value>
                <value>Dubai</value>
                <value>Japan</value>
                <value>China</value>
            </set>
        </property>

        <property name="courses">
            <map>
                <entry key="Science" value="69"/>
                <entry key="Humanities" value="79"/>
                <entry key="SST" value="89"/>
            </map>
        </property>

    </bean>

</beans>
